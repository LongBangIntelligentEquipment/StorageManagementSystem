                    <%for(let j = 0 ; j < component.length ; j++) { %>
                    <tr id="item<%=j%>" style="width: 100%;">
                        <td style="width: 87%">
                            <button class="noteButton" name="itemButton" style="padding-left: 80px;" type="button" onclick="location.href='/adItem?itemId=<%=component[j].itemId%>&returnSql='+document.URL.split('sql=')[1]">
                                <div  style= "font-size: 0.7rem; height: 30px; ">
                                    <span class="itemInfo" style="margin-left: -50px;color: #0050fa;">#<%=j+1%></span>
                                    <span class="itemInfo" >名称：<a style="font-weight: normal;color: #0050fa;"><%=component[j].itemName%></a></span>
                                    <span class="itemInfo" style="margin-left: 260px">物料编号：<a id="itemId" style="font-weight:normal;color: #0050fa; "><%=component[j].itemId%></a></span>
                                </div>
                                <div  style= "font-size: 0.7rem; height: 30px; ">
                                    <span class="itemInfo" >型号（图号）：<a id="itemModel" style="font-weight: normal;color: #0050fa;"><%=component[j].itemModel%></a></span>
                                    <span class="itemInfo" style="margin-left: 350px">单价：<a style="font-weight:normal;color: #0050fa; "><%=component[j].itemPrice%></a></span>
                                </div>
                                <div  style= "font-size: 0.7rem; height: 30px;" id="">
